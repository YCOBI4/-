# -//Вводится последовательность целых чисел, оканчивающаяся нулём, и состоящая более
//чем из одного ненулевого элемента. Определить, являются ли вводимые числа числами
//Фибоначчи
#include <iostream>
using namespace std;

int main()
{
    setlocale(LC_ALL, "Rus");
    int a=0, b=1, c, d, e, f, g; //ввод переменных отвечающих за элементы числовой последовательности
    cout << "Введите числовую последовательность" << endl;
    
    cin >> c; //проверка на количество введенных чисел. Для выполнения программы требуется больше 2 чисел
    if (c == 0 )
    {
        cout << "Введите минимум два значения" << endl;
        return 0;
    }   
    cin >> d;
    cin >> e;
    cin >> f;
    cin >> g;
  


    if (c == a + b && d == b + c && e == c + d && f == d + e && g == e+f) //проверка числовой последовательности на наличие чисел Фибоначчи
    {
                
      cout << "Данные числа являются числами Фибоначчи" << endl;     
        
    }
    else
    {
        cout << "Данные числа не являются числами Фибоначчи " << endl;
    }
